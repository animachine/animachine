{
  "name": "animachine (alpha)",
  "short_name": "animachine",
  "version": "0.0.18",
  "manifest_version": 2,
  "description": "An animation tool for HTML and JS projects.",
  "homepage_url": "https://github.com/animachine/animachine",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "36": "icons/icon36.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png",
    "564": "icons/icon564.png"
  },
  "background": {
    "scripts": ["src/bg/background.js"],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "open animachine"
  },
  "permissions": [
    "storage",
    "clipboardWrite",
    "tabs",
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
